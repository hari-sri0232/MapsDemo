# platform :ios, '9.0'

target 'MapsDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MapsDemo
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Alamofire', '~> 4.7.0'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'Polyline', '~> 4.0'
